  "license": "All rights reserved.",
  "scripts": {
    "clean": "gatsby clean",
    "bootstrap": "yarn build",
    "start": "yarn develop",
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation\" gatsby build",
    "format-all": "yarn format \"**/*.{ts,tsx,scss,json}\"",
    "develop": "cross-env NODE_OPTIONS=\"--no-deprecation\" gatsby develop --port 8008",
    "serve": "gatsby serve",
    "deploy": "gatsby-plugin-s3 deploy --yes",
    "ncu": "ncu",
    "ncu:chakra": "ncu -t greatest '/.*chakra.*/'",
    "lint": "yarn run lint:source && yarn run lint:format",
    "lint:source": "eslint . --ignore-path .gitignore --ext .ts,.tsx,.js,.jsx",
    "lint:format": "prettier --check --ignore-path .gitignore '**/*.{ts,tsx,js,jsx,json}'",
    "format": "prettier --write --ignore-path .gitignore '**/*.{ts,tsx,js,jsx,json}'"
  }
